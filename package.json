{
  "name": "home-chef-delivery",
  "version": "1.0.0",
  "private": true,
  "packageManager": "pnpm@10.0.0",
  "engines": {
    "node": ">=20"
  },
  "devDependencies": {
    "lightningcss": "^1.21.0",
    "typescript": "^5.4.0",
    "eslint": "^8.57.0"
  },
  "scripts": {
    "ci": "pnpm -r test && pnpm -r exec tsc --noEmit",
    "build": "pnpm -r build",
    "lint": "pnpm -r exec eslint .",
    "typecheck": "pnpm --filter=!@home-chef/mobile -r exec tsc --noEmit",
    "test": "pnpm -r test",
    "test:coverage": "pnpm -r test"
  },
  "pnpm": {
    "overrides": {
      "eslint": "9.39.3",
      "@remix-run/node": ">=2.17.2",
      "@remix-run/router": ">=1.23.2",
      "fast-xml-parser": ">=5.3.6",
      "tar": ">=7.5.8",
      "semver": ">=7.5.2",
      "minimatch": ">=9.0.6",
      "esbuild": ">=0.27.3",
      "send": ">=0.19.0",
      "ip": "2.0.1"
    }
  }
}
