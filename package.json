  "version": "1.0.0",
  "private": true,
  "packageManager": "pnpm@10.0.0",
  "engines": {
    "node": ">=20"
  },
  "devDependencies": {
    "lightningcss": "^1.21.0",
    "typescript": "^5.4.0",
    "eslint": "^8.57.0"
