{
  "name": "ridendine",
  "version": "1.0.0",
  "description": "RIDENDINE - Delivery sales tracking dashboard for home cooks",
  "main": "server/index.js",
  "scripts": {
    "dev": "node server/index.js",
    "start": "NODE_ENV=production node server/index.js",
    "test": "jest --testTimeout=10000",
    "test:watch": "jest --watch",
    "migrate": "node-pg-migrate up",
    "migrate:down": "node-pg-migrate down",
    "migrate:create": "node-pg-migrate create",
    "seed": "node seed.js"
  },
  "keywords": [
    "delivery",
    "food",
    "platform"
  ],
  "author": "RideNDine",
  "license": "MIT",
  "dependencies": {
    "bcrypt": "^6.0.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.6",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.5.1",
    "jsonwebtoken": "^9.0.3",
    "node-pg-migrate": "^8.0.4",
    "pg": "^8.18.0",
    "stripe": "^14.10.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "devDependencies": {
    "jest": "^30.2.0",
    "supertest": "^7.2.2"
  },
  "jest": {
    "testEnvironment": "node",
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/docs/"
    ],
    "testMatch": [
      "**/__tests__/**/*.test.js"
    ]
  }
}
